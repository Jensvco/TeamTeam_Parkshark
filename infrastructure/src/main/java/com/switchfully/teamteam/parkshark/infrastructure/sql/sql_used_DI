   create table DIRECTOR(
   ID number not null,
   FIRST_NAME varchar2(25) not null,
   LAST_NAME varchar2(25) not null,
   constraint PK_ID primary key (id)
   );

   create table DIVISION
   (
   ID number not null,
   NAME varchar2(25) not null,
   ORIGINAL_NAME varchar2(25) not null,
   DIRECTOR_ID number not null,
   constraint PK_DIVISION primary key (ID),
   constraint FK_DIRECTOR_ID foreign key (DIRECTOR_ID) references DIRECTOR (ID)

   );


   CREATE SEQUENCE  "HRSTUDENT7"."PARKSHARK_DIRECTOR_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE ;
   CREATE SEQUENCE  "HRSTUDENT7"."PARKSHARK_DIVISION_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE ;
   CREATE SEQUENCE  "HRSTUDENT7"."PARKSHARK_PARKINGLOT_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE ;


ALTER TABLE DIVISION
ADD PARENT_DIVISION_ID number;


ALTER TABLE DIVISION
ADD CONSTRAINT DIVISION_FK1 FOREIGN KEY
( PARENT_DIVISION_ID )
REFERENCES DIVISION( ID ) /*ENABLE*/;